<p id="notice"><%= notice %></p>

<h1>Listing Prenatals</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prenatals.each do |prenatal| %>
      <tr>
        <td><%= link_to 'Show', prenatal %></td>
        <td><%= link_to 'Edit', edit_prenatal_path(prenatal) %></td>
        <td><%= link_to 'Destroy', prenatal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prenatal', new_prenatal_path %>
